{
  "ac/input/one": {
    "prefix": "ac/input/one",
    "body": [
      "$0a = int(input())"
    ],
    "description": "intの入力"
  },
  "ac/input/multi": {
    "prefix": "ac/input/multi",
    "body": [
      "$0a, b = map(int, input().split())"
    ],
    "description": "複数のintの入力"
  },
  "ac/input/list": {
    "prefix": "ac/input/list",
    "body": [
      "$0a = list(map(int, input().split()))"
    ],
    "description": "intのlistの入力"
  },
  "ac/input/char_list": {
    "prefix": "ac/input/char_list",
    "body": [
      "$0a = list(input())[:-1]"
    ],
    "description": "文字の配列の入力(input = sys.stdin.readlineの場合)"
  },
  "ac/input/str": {
    "prefix": "ac/input/str",
    "body": [
      "$0a = input()[:-1]"
    ],
    "description": "文字列の入力(input = sys.stdin.readlineの場合)"
  }
}
